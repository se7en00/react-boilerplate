{
    "parser": "babel-eslint",
    "extends": "react-app",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },

    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "globals": {
        "__dirname": false,
        "require": false,
        "document": false,
        "window": false,
        "console": false,
        "module": false,
        "R": true,
        "moment": true
    },
    "rules": {
        "no-console": 2
    },
    "plugins": [
        "babel",
        "react",
        "import"
    ]
}
